<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <body>
       <referenceBlock name="product.info.details">
           <block class="Magento\Catalog\Block\Product\View" name="custom.tab" template="TestTab_Tab::custom_tab.phtml" group="detailed_info" >
               <arguments>
                   <argument translate="true" name="title" xsi:type="string">Shiiping Info</argument>
                   <argument name="sort_order" xsi:type="string">10</argument>
               </arguments>
           </block>
       </referenceBlock>
        <referenceBlock name="reviews.tab">
            <arguments>
                <argument name="sort_order" xsi:type="string">1</argument>
            </arguments>
        </referenceBlock>
       
   </body>
</page>